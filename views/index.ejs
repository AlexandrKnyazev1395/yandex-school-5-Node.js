<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>MyGit</title>
    <script src='/scripts/bundle.js'> </script>
  </head>
  <body>
    <h1> <a href='/'><%= title %></a></h1>
    <h2><%= section %> </h2>
    <ul class="sources-list">
      <% branches.forEach(function(branch){ %>
        <li>
          <span><%= branch %></span> 
          <a href="commits/<%= branch %>">commits</a> 
          <a href="sources/<%= branch %>">sources</a> 
        </li>
      <% }); %>
    </ul>
  </body>
</html>
